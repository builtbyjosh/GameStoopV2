<h1>Changing <%=current_user.username %>'s Profile</h1>

<%= form_for(current_user) do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email%>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password%>
    <br>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
    <br>
    <%= f.submit %>
<% end %>